									</tr>
									<tr>
										<td><input tabindex="3" style="margin: 5 0 0 0"
											type='image' alt="Войти"
											src="/images/__backend/common/but-ok.gif"></td>
									</tr>
								</table>
								</form>

								</td>
							</tr>
						</table>

						</td>
					</tr>
				</table>

				</td>
			</tr>
			<tr>
				<td width="100%" colspan="3"><div class="auth-bot"></div></td>
			</tr>
		</table>
		</td>
	</tr>
